<%
	for (int i = 0; i < 5; i++) {
		for (int j= 0 ; j < 5; j++) {
			out.print("*");
		}
		out.print("<br>");
	}
%>

<%
	// 구구단 출력
	out.print("<table border='1' >");
	out.print("<tr>");
	
		for (int i = 1; i < 10; i++) {
			for (int j = 1; j < 10; j++) {
			out.print("<th>" + i + "단" + "</th>");
			out.print("<td>" + i + "x" + j + "=" + i*j + "<br>" + "<td>");
		}
		//out.print("<br>");
		out.print("</table>");
	}
	out.print("<tr>");
%>

<%
	for (int i = 1; i < 10; i++) {
		out.print("<td>" + i + "단" + "</td>");
	}

%>

